#%Module######################################################################
##
##      ANSYS modulefile
##
proc ModulesHelp { } {
    puts stderr "Set environment for ANSYS $version"
}

set base_path /share/apps/opt/ansys
set version   13

module-whatis "Setup ANSYS $version"

set path $base_path/$version

prepend-path PATH $path/v130/ansys/bin
