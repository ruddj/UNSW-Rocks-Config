#%Module######################################################################
##
##      FLUENT modulefile
##
proc ModulesHelp { } {
    puts stderr "Set environment for FLUENT $version"
}

set base_path /share/apps/opt/ansys
set version   14

module-whatis "Setup FLUENT $version"

set path $base_path/$version

prepend-path PATH $path/v140/fluent/bin
